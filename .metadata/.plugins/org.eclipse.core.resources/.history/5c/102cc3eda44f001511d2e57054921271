package com.streaming.cummulativeSum;

import org.apache.spark.SparkConf;
import org.apache.spark.api.java.JavaSparkContext;
import org.apache.spark.streaming.api.java.JavaStreamingContext;

public class LogAnalyserStreamingTotal {
	SparkConf conf=new SparkConf().setAppName("Log Analyser Streaming Total");
	JavaSparkContext cntx=new JavaSparkContext(conf);
	JavaStreamingContext jssc=new JavaStreamingContext(cntx,1000);  
}	
